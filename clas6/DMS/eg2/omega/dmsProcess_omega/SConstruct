import os
env = Environment(ENV = os.environ)
env.ParseConfig('root-config --cflags --glibs')
env.Append(CPPPATH=['/home/beitera/evioRoot/src/root','/home/beitera/evioRoot/src/writer','/home/beitera/evioRoot/src/tools'])
env.Append(LIBPATH=['/home/beitera/evioRoot/lib/'])
env.Append(LIBS=["EvioRoot"])
t = env.Program(target='dmsProcess_omega', source=['dmsProcess_omega.cc', 'DetectedParticles.cc', 'IntermediateParticles.cc', 'ReconstructedParticles.cc', 'ParticleList.cc', 'EG2Target.cc', 'EG2Cuts.cc', 'ElectronID.cc', 'EC_geometry.cc', 'OmegaMixedEvent.cc'])
Default(t)
